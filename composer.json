{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/mattdfloyd/echosign"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.4.*",
        "laravelcollective/html": "5.4.*",
        "sameer-shelavale/php-countries-array": "*",
        "stripe/stripe-php": "~3.5",
        "bugsnag/bugsnag-laravel": "1.*",
        "itbz/fpdf": "~1.7",
        "guzzlehttp/guzzle": "~6.0",
        "mandrill/mandrill": "~1.0",
        "barryvdh/laravel-debugbar": "~2.1",
        "nsbucky/echosignv3": "dev-master",
        "adam-paterson/oauth2-stripe": "~1.1",
        "wildbit/postmark-php": "~2.2",
        "league/csv": "~8.1",
        "jenssegers/rollbar": "~1.5",
        "graham-campbell/flysystem": "~3.4",
        "league/flysystem-dropbox": "~1.0",
        "sofa/revisionable": "5.3.*",
        "maatwebsite/excel": "~2.1",
        "vinkla/pusher": "~2.3",
        "spipu/html2pdf": "~4.6",
        "codedge/laravel-fpdf": "~1.0",
        "printnode/printnode-php": "2.0.0-rc1",
        "laravel/tinker": "~1.0",
        "zondicons/blade-bridge": "^0.1.1",
        "sentry/sentry-laravel": "^0.6.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
